{
  "targets": {
    "browser": {
      "compile": [
        {
          "input": "src/*.html",
          "output": "docs/",
          "minify": true
        },
        {
          "input": "src/css/main-cssvars.scss",
          "output": "docs/css/main.css",
          "minify": true
        },
        {
          "input": "src/js/main.js",
          "output": {
            "file": "docs/js/main.js",
            "format": "umd"
          },
          "globals": {
            "gsap": "gsap"
          },
          "minify": true
        }
      ],
      "bundle": [],
      "copy": [{
        "input": "src/modals/**/*.html",
        "output": "docs/modals/"
      }]
    },
    "vendors": {
      "compile": [],
      "bundle": [
        {
          "input": [
            "node_modules/gsap/dist/EasePack.js",
            "node_modules/gsap/dist/gsap.js"
          ],
          "output": "docs/js/vendors.js",
          "minify": true
        }
      ],
      "copy": []
    }
  },
  "tfs": false,
  "server": {
    "root": "./docs",
    "path": "/tm-urban-contest/",
    "host": "0.0.0.0",
    "port": 49876,
    "log": false
  }
}
